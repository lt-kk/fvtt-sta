    "@types/jquery": "3.5.29",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.1",
    "handlebars": "4.7.8",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "lodash": "^4.17.21",
    "prettier": "3.1.1",
    "rollup": "4.9.1",
